<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity ShoppingCartItems.
    -->
    <changeSet id="20190801044955-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="product_id"
                                 baseTableName="shopping_cart_items"
                                 constraintName="fk_shopping_cart_items_product_id"
                                 referencedColumnNames="id"
                                 referencedTableName="products"/>

        <addForeignKeyConstraint baseColumnNames="cart_id"
                                 baseTableName="shopping_cart_items"
                                 constraintName="fk_shopping_cart_items_cart_id"
                                 referencedColumnNames="id"
                                 referencedTableName="shopping_carts"/>

    </changeSet>
</databaseChangeLog>
